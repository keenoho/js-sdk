{
	"name": "@keenoho/js-core",
	"version": "0.0.1",
	"description": "keenoho js core",
	"main": "./index.js",
	"scripts": {
		"dev": "rollup -c ./rollup.config.js --bundleConfigAsCjs --watch",
		"build": "rollup -c ./rollup.config.js --bundleConfigAsCjs",
		"beforePublish": "node ./build/publish.js && npm run fmt && npm run build",
		"fmt": "prettier -w ./src/**/*.*",
		"publish": "npm publish --access public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/keenoho/js-sdk.git"
	},
	"author": "keenoho",
	"license": "ISC",
	"homepage": "https://github.com/keenoho/js-sdk.git",
	"exports": {
		".": {
			"types": {
				"require": "./index.d.ts",
				"default": "./index.d.ts"
			},
			"browser": {
				"require": "./dist/browser/sdk.umd.js",
				"default": "./index.js"
			},
			"default": {
				"require": "./dist/sdk.js",
				"default": "./index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"devDependencies": {
		"@babel/core": "^7.21.4",
		"@babel/preset-env": "^7.21.4",
		"@babel/runtime": "^7.21.0",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^24.1.0",
		"@rollup/plugin-json": "^6.0.0",
		"@rollup/plugin-node-resolve": "^15.0.2",
		"@rollup/plugin-terser": "^0.4.1",
		"cross-env": "^7.0.3",
		"prettier": "^2.8.8",
		"rollup": "^3.20.7",
		"rollup-plugin-auto-external": "^2.0.0"
	},
	"dependencies": {
		"axios": "^1.3.6",
		"crypto-js": "^4.1.1",
		"md5": "^2.3.0",
		"uuid": "^9.0.0"
	},
	"files": [
		"./dist/*",
		"./src/**/*.*",
		"./index.js",
		"./package.json",
		"./index.d.ts"
	]
}
